{
  "name": "taxi-gestion-client",
  "version": "1.0.0",
  "description": "Description",
  "main": "build/index.js",
  "scripts": {
    "build": "ng build",
    "lint.commit": "mv tsconfig.json tsconfig.jso && commitlint --from $(git rev-list --max-parents=0 HEAD | head -1) --to HEAD --config .commitlint.config.cjs ; mv tsconfig.jso tsconfig.json",
    "lint.es": "ng lint",
    "lint.style": "stylelint ./**/*.scss",
    "ng": "ng",
    "prepare": "husky install && chmod -R +x .husky",
    "prettier.check": "prettier --check --ignore-unknown .",
    "prettier.fix": "prettier --write --ignore-unknown .",
    "start": "ng serve",
    "start-expose": "ng serve --host 0.0.0.0",
    "test": "vitest --run",
    "watch": "ng build --watch --configuration development"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/taxi-gestion/client.git"
  },
  "keywords": [
    "taxi-gestion",
    "api"
  ],
  "author": "taxi-gestion",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taxi-gestion/client/issues"
  },
  "homepage": "https://github.com/taxi-gestion/client#readme",
  "dependencies": {
    "@angular/animations": "^17.1.2",
    "@angular/common": "^17.1.2",
    "@angular/compiler": "^17.1.2",
    "@angular/core": "^17.1.2",
    "@angular/forms": "^17.1.2",
    "@angular/platform-browser": "^17.1.2",
    "@angular/platform-browser-dynamic": "^17.1.2",
    "@angular/router": "^17.1.2",
    "@angular/service-worker": "^17.1.2",
    "cron-parser": "^4.9.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "fp-ts": "^2.16.2",
    "io-ts": "^2.2.21",
    "io-ts-excess": "^1.0.1",
    "rrule": "^2.8.1",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "xlsx": "0.18.5",
    "zone.js": "^0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.2",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "^17.2.1",
    "@angular-eslint/eslint-plugin-template": "17.2.1",
    "@angular-eslint/schematics": "17.2.1",
    "@angular-eslint/template-parser": "17.2.1",
    "@angular/cli": "~17.1.2",
    "@angular/compiler-cli": "^17.1.2",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@popperjs/core": "^2.11.8",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@tsconfig/esm": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "bootstrap": "^5.3.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.34",
    "prettier": "^3.2.5",
    "semantic-release": "^23.0.0",
    "stylelint": "^16.2.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
